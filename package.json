{
  "name": "notes",
  "version": "0.0.1",
  "private": true,
  "description": "Computer science learn notes",
  "author": "with-rainbow",
  "license": "MIT",
  "repository": "https://github.com/with-rainbow/notes",
  "main": "index.ts",
  "scripts": {
    "build": "pnpm --parallel build && pnpm docs:build",
    "build-watch": "pnpm --parallel -r build-watch",
    "clean": "rimraf docs/.vitepress/dist && pnpm --parallel -r clean",
    "dev": "run-p build-watch docs:dev",
    "docs:build": "vitepress build docs --clean-cache",
    "docs:dev": "vitepress dev docs --clean-cache",
    "check": "lychee -c lychee.toml -f markdown docs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "serve docs/.vitepress/dist",
    "type": "pnpm --parallel -r --stream type",
    "up": "taze -rwI -x '@rainbowatcher'"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@rainbowatcher/markdown-it-katex": "workspace:0.0.1",
    "@rainbowatcher/theme-sika": "workspace:0.0.1",
    "@rainbowatcher/vitepress-plugin-auto-sidebar": "workspace:0.0.1",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.11.0",
    "@unocss/preset-attributify": "^0.45.29",
    "@unocss/preset-icons": "^0.45.29",
    "@unocss/preset-typography": "^0.45.29",
    "@unocss/preset-uno": "^0.45.29",
    "@unocss/preset-web-fonts": "^0.45.29",
    "@unocss/transformer-directives": "^0.45.29",
    "eslint": "^8.25.0",
    "markdown-it-deflist": "^2.1.0",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-ins": "^3.0.1",
    "markdown-it-mark": "^3.0.1",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-task-lists": "^2.1.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "taze": "^0.8.2",
    "tsup": "^6.2.3",
    "typescript": "^4.8.4",
    "unocss": "^0.45.29",
    "vitepress": "^1.0.0-alpha.21",
    "vue": "^3.2.41"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "vite"
      ]
    }
  }
}
